        <List>
          <String>approved</String>
          <String>report_approved</String>
        </List>
        <List>
          <String>rejected</String>
          <String>report_rejected</String>
        </List>
        <List>
          <String>pending</String>
          <String>report_pending</String>
        </List>
        <List>
          <String>canceled</String>
          <String>report_canceled</String>
