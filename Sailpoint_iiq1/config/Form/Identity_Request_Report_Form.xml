    <Field columnSpan="1" displayName="report_requesters" helpKey="rept_input_lcm_request_status_report_requester" multi="true" name="requestors" type="Identity" value="ref:requestors"/>
    <Field columnSpan="1" displayName="report_target_identities" helpKey="rept_input_lcm_request_status_report_target_identity" multi="true" name="targetIdentities" type="Identity" value="ref:targetIdentities"/>
