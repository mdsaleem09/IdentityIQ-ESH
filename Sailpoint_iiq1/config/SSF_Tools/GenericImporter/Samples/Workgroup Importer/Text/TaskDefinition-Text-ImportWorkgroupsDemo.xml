<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE TaskDefinition PUBLIC "sailpoint.dtd" "sailpoint.dtd">
<TaskDefinition name="Generic Importer - Import Workgroups demo - Text File" resultAction="Delete" subType="task_item_type_generic" type="Generic">
  <Attributes>
    <Map>
      <entry key="genericImportDriverClass" value="TextFileImport"/>
      <entry key="importLoggerName" value="sailpoint.services.task.workgroupDemo"/>
      <entry key="importRowRule" value="WorkgroupImporter_Row"/>
      <entry key="taskCompletionEmailNotify" value="Disabled"/>
      <entry key="taskCompletionEmailRecipients"/>
      <entry key="taskCompletionEmailTemplate"/>
      <entry key="text_fileDelimiter" value=","/>
      <entry key="text_fileName" value="{Eneter correct path}{file path}\GenericImporter\config\SSF_Tools\GenericImporter\Samples\Workgroup Importer\Text\workgroups.csv"/>
      <entry key="text_hasHeader" value="true"/>
      <entry key="importLoggerLevel" value="debug" />
      <entry key="importManualHeader" value="workgroup_name(+Identity),workgroup_owner(Identity)" />
    </Map>
  </Attributes>
  <Description>Generic Import Task
Import workgroups and their members</Description>
  <Owner>
    <Reference class="sailpoint.object.Identity" name="spadmin"/>
  </Owner>
  <Parent>
    <Reference class="sailpoint.object.TaskDefinition" name="Generic Importer"/>
  </Parent>
</TaskDefinition>
